{
  "name": "website",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-2"
          ]
        }
      ]
    ]
  },
  "version": "1.0.0",
  "description": "a site",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production browserify index.js | uglifyjs -cm > assets/bundle.js",
    "watch": "watchify index.js -o assets/bundle.js -dv",
    "watch-map": "watchify assets/js/map/index.js -o assets/js/map/bundle.js -dv",
    "serve": "bundle exec jekyll serve",
    "killjekyll": "pkill -f jekyll"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "browserify": "^10.2.6",
    "eases": "^1.0.8",
    "glfx": "0.0.4",
    "npm": "^6.3.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scroll-parallax": "^1.3.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "simple_animation_loop": "^1.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.0.1",
    "reactify": "^1.1.1",
    "uglify-js": "^2.8.29",
    "watchify": "^3.11.0"
  }
}
